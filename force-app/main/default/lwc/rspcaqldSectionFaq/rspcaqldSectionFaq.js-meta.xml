<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>64.0</apiVersion>
    <description>Section FAQ</description>
    <isExposed>true</isExposed>
    <masterLabel>Section FAQ</masterLabel>
    <targets>
        <target>lightningCommunity__Default</target>
        <target>lightningCommunity__Page</target>
    </targets>

    <targetConfigs>
        <targetConfig targets="lightningCommunity__Default">
            <property name="customClass" type="String" label="Custom Class"/>
            <property name="header" type="String" label="Header" default="Frequently asked questions"/>
            <property name="linkLabel" type="String" label="Link Label" default="Browse all FAQs"/>
            <property name="linkUrl" type="String" label="Link URL"/>
            <property name="faqsString" type="String" label="FAQs"/>
            <property name="isWhite" type="Boolean" label="White Background"/>
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>