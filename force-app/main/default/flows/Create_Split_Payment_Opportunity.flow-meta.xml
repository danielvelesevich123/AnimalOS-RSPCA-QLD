        <expression>{!splitPaymentOpportunityAmount}+{!gift.Amount}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>updateAmountRecurring</name>
        <dataType>Currency</dataType>
        <expression>{!splitPaymentOpportunityAmount}+{!recurringDonation.npe03__Amount__c}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>UpdateName</name>
        <dataType>String</dataType>
        <expression>{!ContactName} +&quot; $&quot;+TEXT({!UpdateAmount})+&quot; Donation &quot;+ TEXT(DAY({!PaymentOpportunity.CloseDate}))+&quot;/&quot; +TEXT(MONTH({!PaymentOpportunity.CloseDate}))+&quot;/&quot; +TEXT(YEAR({!PaymentOpportunity.CloseDate}))</expression>
    </formulas>
