    <formulas>
        <name>plusAmount</name>
        <dataType>Currency</dataType>
        <expression>{!gift.Amount} + IF(
  NOT(ISNULL({!donation.Amount})),
  {!donation.Amount},
  {!paymentVar.npsp_plus__Payment_Fee__c}
)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>SplitPaymentOpportunityNameFormula</name>
        <dataType>String</dataType>
        <expression>{!PaymentOpportunity.Primary_Contact_Full_Name__c} &amp;
&quot; &quot;&amp;
&quot;$&quot;&amp;
TEXT({!UpdateAmount}) &amp;
&quot; &quot; &amp;
&quot;Split Payment &quot; &amp;
TEXT(DAY({!$Flow.CurrentDate})) &amp;
&quot;/&quot; &amp;
TEXT(MONTH({!$Flow.CurrentDate})) &amp; 
&quot;/&quot; &amp;
TEXT(YEAR({!$Flow.CurrentDate}))</expression>
    </formulas>
    <formulas>
        <name>UpdateAmount</name>
