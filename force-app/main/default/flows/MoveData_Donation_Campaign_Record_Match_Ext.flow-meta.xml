    <description>Updated FinancialYearTeam to include support for additional previous financial years</description>
    <environments>Default</environments>
    <formulas>
        <name>FinancialYearDonation</name>
        <dataType>String</dataType>
        <expression>IF(
    AND(DATEVALUE({!Donation_StartDate}) &gt;= DATE(2022,07,01),DATEVALUE({!Donation_StartDate}) &lt;= DATE(2023,06,30)),&apos;FY22/23&apos;,
    (
        IF((AND(DATEVALUE({!Donation_StartDate}) &gt;= DATE(2023,07,01),DATEVALUE({!Donation_StartDate}) &lt;= DATE(2024,06,30))),&apos;FY23/24&apos;,
        (
            IF((AND(DATEVALUE({!Donation_StartDate}) &gt;= DATE(2024,07,01),DATEVALUE({!Donation_StartDate}) &lt;= DATE(2025,06,30))),&apos;FY24/25&apos;,
            (
                IF((AND(DATEVALUE({!Donation_StartDate}) &gt;= DATE(2025,07,01),DATEVALUE({!Donation_StartDate}) &lt;= DATE(2026,06,30))),&apos;FY25/26&apos;,
                (
                    IF((AND(DATEVALUE({!Donation_StartDate}) &gt;= DATE(2026,07,01),DATEVALUE({!Donation_StartDate}) &lt;= DATE(2027,06,30))),&apos;FY26/27&apos;,
                    &apos;&apos;)
                ))
            ))
        ))
    )
)</expression>
    </formulas>
    <formulas>
