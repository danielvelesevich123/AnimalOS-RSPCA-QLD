<apex:page id="AdoptionAgreement"
           controller="aos_AdoptionAgreementCtrl"
           renderAs="pdf"
           applyBodyTag="false"
           applyHtmlTag="false"
           showHeader="false" sidebar="false" standardStylesheets="false"
           action="{!initMetadata}">


    <head>
        <title>Adoption Agreement</title>
        <c:aos_AdoptionAgreementStyles />
    </head>

    <apex:form >
        <apex:pageBlock id="myBlock">

            <div class="main-content">
                <header>
                    <table>
                        <tr>
                            <td class="header-table" width="33%">
                                <apex:image id="RspcaLogo" value="{!$Resource.RspcaLogo}" width="220" alt="RSPCA QLD"/>
