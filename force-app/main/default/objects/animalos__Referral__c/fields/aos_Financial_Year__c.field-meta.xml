<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>aos_Financial_Year__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
    MONTH(DATEVALUE(animalos__Scheduled_Date_Time__c)) &gt;= 7, 
    TEXT(YEAR(DATEVALUE(animalos__Scheduled_Date_Time__c)) + 1), 
    TEXT(YEAR(DATEVALUE(animalos__Scheduled_Date_Time__c)))
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
